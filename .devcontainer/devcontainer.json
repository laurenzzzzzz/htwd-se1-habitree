{
  "name": "Habitree Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/htwd-se1-habitree",
  "settings": {},
  "extensions": [
    "dbaeumer.vscode-eslint",
    "GitHub.copilot-chat",
    "esbenp.prettier-vscode",
    "asciidoctor.asciidoctor-vscode"
  ],
  "forwardPorts": [8081, 19000, 19001, 19002, 19006],
  "postCreateCommand": "cd src/habitree && npm ci && npx expo install && npx expo start --tunnel --go --clear"
}
